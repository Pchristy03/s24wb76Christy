extends layout

block content
  h1= title
  - var image_names = ["fig1.jpeg", "fig2.jpeg", "fig3.jpg", "fig4.jpeg", "fig5.webp"]
  - for (var i=0; i<3; i++) {
    - plant = image_names[Math.floor(Math.random()*5)]
    img(src=`/images/${plant}`, alt=`/images/${plant}`, height="300", width="300")
  - }

